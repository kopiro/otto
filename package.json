{
  "name": "otto-ai",
  "version": "2.0.0",
  "description": "Simple AI for Otto",
  "workspaces": [
    "./src/packages/*"
  ],
  "private": true,
  "engines": {
    "node": ">=8 <9"
  },
  "scripts": {
    "prod": "yarn pm2 start ./ecosystem.config.js --no-daemon",
    "dev": "yarn pm2-dev start ./ecosystem.config.js",
    "unit-test": "jest ./src --passWithNoTests",
    "lint": "eslint --fix ./src",
    "test": "yarn lint && yarn unit-test",
    "build": "docker-compose build",
    "deploy": "yarn build && docker-compose push"
  },
  "os": [
    "darwin",
    "linux"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kopiro/otto-ai.git"
  },
  "author": "Flavio De Stefano <destefano.flavio@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/kopiro/otto-ai#readme",
  "bugs": {
    "url": "https://github.com/kopiro/otto-ai/issues"
  },
  "dependencies": {
    "@google-cloud/speech": "^0.10.3",
    "@google-cloud/storage": "^1.7.0",
    "@google-cloud/text-to-speech": "^0.3.0",
    "@google-cloud/translate": "^1.0.0",
    "assign-deep": "^1.0.1",
    "aws-sdk": "^2.349.0",
    "body-parser": "^1.18.3",
    "console-ultimate": "^2.9.0",
    "deep-extend": "^0.5.1",
    "diacritics": "^1.3.0",
    "dialogflow": "^0.7.0",
    "express": "^4.16.4",
    "fast-levenshtein": "^2.0.6",
    "fb": "^2.0.0",
    "gm": "^1.23.1",
    "google-images": "^2.0.0",
    "keypress": "^0.2.1",
    "md5": "^2.2.1",
    "moment": "^2.22.2",
    "mongoose": "^5.8.2",
    "mongoose-autopopulate": "^0.10.0",
    "node-telegram-bot-api": "^0.30.0",
    "pm2": "^4.2.1",
    "raven": "^2.6.4",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "socket.io": "^2.1.1",
    "transmission": "^0.4.9",
    "underscore": "^1.9.1",
    "uuid": "^3.3.2",
    "yargs": "^15.3.0"
  },
  "devDependencies": {
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^23.1.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.12.4",
    "jest": "^24.9.0",
    "prettier": "^1.19.1"
  },
  "optionalDependencies": {
    "apa102-spi": "0.0.3",
    "rpio": "^0.9.19",
    "snowboy": "^1.2.0"
  }
}
