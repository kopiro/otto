{
  "name": "otto",
  "version": "3.0.0",
  "description": "Simple AI for Otto",
  "engine": ">=13",
  "bin": {
    "otto": "build/cli.ts"
  },
  "main": "build/index.js",
  "private": true,
  "scripts": {
    "build": "tsc",
    "start": "node build",
    "start:pi": "play -v 0.1 ./etc/boot.wav; pnpm run start",
    "start:dev": "nodemon",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "pnpm run lint --fix",
    "unitest": "jest src --passWithNoTests",
    "test": "pnpm run lint && pnpm run unitest",
    "docker:build": "docker build . -t kopiro/otto",
    "docker:run": "docker run -it kopiro/otto",
    "docker:deploy": "pnpm run build:docker && docker push kopiro/otto",
    "ai:memory": "ts-node ./src/cli/memory.ts",
    "ai:born": "MEMORY_TYPE=declarative,episodic,social pnpm run ai:memory",
    "ai:sleep": "MEMORY_TYPE=episodic pnpm run ai:memory",
    "ai:social": "MEMORY_TYPE=social pnpm run ai:memory",
    "ai:finetune": "ts-node ./src/cli/finetune.ts"
  },
  "os": [
    "darwin",
    "linux"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kopiro/otto.git"
  },
  "author": "Flavio De Stefano <destefano.flavio@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/kopiro/otto#readme",
  "bugs": {
    "url": "https://github.com/kopiro/otto/issues"
  },
  "dependencies": {
    "@google-cloud/speech": "^5.5.0",
    "@google-cloud/storage": "^5.0.0",
    "@google-cloud/text-to-speech": "^4.2.2",
    "@google-cloud/translate": "^6.3.1",
    "@picovoice/porcupine-node": "^2.2.1",
    "@picovoice/pvrecorder-node": "^1.1.2",
    "@qdrant/js-client-rest": "^1.2.2",
    "@qdrant/qdrant-js": "^1.2.2",
    "@sentry/node": "^5.15.4",
    "@typegoose/typegoose": "^11.2.0",
    "body-parser": "^1.20.1",
    "detect-rpi": "^1.4.0",
    "dotenv": "^8.2.0",
    "express": "^4.18.2",
    "express-basic-auth": "^1.2.1",
    "express-rate-limit": "^7.5.0",
    "formidable": "^2.1.1",
    "google-auth-library": "^8.8.0",
    "google-images": "^2.1.0",
    "moment": "^2.29.4",
    "mongoose": "^7.3.0",
    "mongoose-autopopulate": "^0.16.0",
    "node-fetch": "^2",
    "node-record-lpcm16": "^1.0.1",
    "node-telegram-bot-api": "^0.66.0",
    "openai": "^4.87.3",
    "readline": "^1.3.0",
    "signale": "^1.4.0",
    "ts-node": "^10.9.1",
    "typed-emitter": "^2.1.0",
    "typescript": "^5.0.4",
    "uuid": "^8.3.2",
    "uuid-by-string": "^4.0.0",
    "wav-file-info": "^0.0.10"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/express": "^4.17.17",
    "@types/formidable": "^2.0.6",
    "@types/google-images": "^2.1.1",
    "@types/jest": "^26.0.20",
    "@types/mocha": "^8.2.1",
    "@types/mongoose-autopopulate": "^0.10.1",
    "@types/node": "^18.7.16",
    "@types/node-fetch": "^2.5.10",
    "@types/node-telegram-bot-api": "^0.64.8",
    "@types/pumpify": "^1.4.1",
    "@types/signale": "^1.4.4",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "5.0.0",
    "eslint": "^8.29.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.5",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.3"
  },
  "packageManager": "pnpm@9.15.2+sha1.0ffb02f94047016ec7e088030337455b4c94bb34"
}
